{
	"name": "huggingface-sync",
	"author": "JacobLinCool <jacob@csie.cool> (https://jacoblin.cool)",
	"license": "MIT",
	"version": "1.3.1",
	"description": "Sync your GitHub repository to Hugging Face Spaces.",
	"files": [
		"dist",
		"action.yml",
		"README.md"
	],
	"scripts": {
		"prepare": "husky",
		"format": "prettier --write --ignore-path .gitignore .",
		"test": "jest --coverage",
		"changeset": "changeset",
		"build": "esbuild src/index.ts --platform=node --bundle --outfile=dist/index.js"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.11",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.5",
		"esbuild": "^0.24.2",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^15.3.0",
		"prettier": "^3.4.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"serve": "^14.2.4",
		"ts-jest": "^29.2.5",
		"tsup": "^8.3.5",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml}": [
			"prettier --write"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JacobLinCool/huggingface-sync.git"
	},
	"bugs": {
		"url": "https://github.com/JacobLinCool/huggingface-sync/issues"
	},
	"homepage": "https://jacoblincool.github.io/huggingface-sync",
	"packageManager": "pnpm@10.6.5",
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@actions/github": "^6.0.0",
		"fs-extra": "^11.2.0"
	}
}
